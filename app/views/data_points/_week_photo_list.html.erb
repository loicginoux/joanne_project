<div class="row">
	<div class="span6">
		<h2>{{startDate}} - {{endDate}} {{year}}</h2>
	</div>
</div>
<div class="row">
{{#data}}
	<div class="span1_7 {{#first}}first{{/first}}">
	<h5 class="pagination-centered">{{day_date}}</h5>
	<br/>
	{{#data_points}}
		<div class="image">
			<img class="thumb" data-original-title="{{uploaded_at_readable}}" data-content="Calories: {{calories}}" src="http://s3.amazonaws.com/<%= S3_CREDENTIALS[:bucket]%>/photos/{{img_path}}" />
			<div class="edit_data_point hide " data-toggle="modal" data-target="#modal_{{id}}">
				<i class="icon-pencil"></i>
			</div>
		</div>
		<%= render "data_points/update_modal_box" %>
		
	{{/data_points}}

	{{^data_points}}
		<div class="image">
			<p class="pagination-centered"></p>
		</div>	
	{{/data_points}}
	</div>
{{/data}}
{{^data}}
<div class="alert alert-info">
	<button class="close" data-dismiss="alert">Ã—</button>
	<strong>No photos!</strong>
</div>
{{/data}}
</div>
