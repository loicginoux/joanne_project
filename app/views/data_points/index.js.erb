<% if @period == "month" %>
	$("#photos").append("<%= escape_javascript(render :partial => 'data_points/index/month_photo_list') %>");
<% elsif @period == "week" %>
	$("#photos").append("<%= escape_javascript(render :partial => 'data_points/index/week_photo_list') %>");
<% elsif @period == "day" %>
	$("#photos").append("<%= escape_javascript(render :partial => 'data_points/index/day_photo_list') %>");
<% end %>

<% if @data_points.length > 0 %>
	window.Spine.trigger('data_points:loaded', <%= @graphicDatas %>, '<%= @period %>')
<% end %>
